    <!--Project 2: On the Rocks-->
    <div class="two-col project-index">
        <div class="container__project-image">
          <img src="<%= image_path("images/project-ice/ice-all.JPG") %>" alt="on the rocks" class="img-project">
        </div>
        <div class="container__project-text">
            <h3 class="default-spec-color"><strong>↓ PROJECT NO. 02</strong></h3><br>
            <div class="project__text-title"><div class="default-color"><strong>On the Rocks</strong></div>
